package builder;

public class HawaianPizzaBuilder extends PizzaBuilder {
	private Pizza pizza;
	
	@Override
	public void setName() {
		this.pizza.setName("Hawaian Pizza");
	}

	@Override
	public void setMasa() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void setSalsa() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public void setIngredientes() {
		// TODO Auto-generated method stub
		
	}

	@Override
	public Pizza getPizza() {
		return this.pizza;
	}

	@Override
	public void startPizza(Pizza newPizza) {
		this.pizza = newPizza;		
	}

}
