package builder;

public class Client {
	public static void main(String[] args) {
		Cocina cocina = new Cocina();
		PizzaBuilder hawaianPizzaBuilder = new HawaianPizzaBuilder();
		cocina.setPizzaBuilder(hawaianPizzaBuilder);
		cocina.buildPizza();
		Pizza hawaianPizza1 = cocina.getPizza();
		System.out.println(hawaianPizza1.toString());
		cocina.buildPizza("H2");
		Pizza hawaianPizza2 = cocina.getPizza();
		System.out.println(hawaianPizza2.toString());
	}
}
